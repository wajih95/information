{
  "functions": {
    "dist/bundle.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/bundle.js"
    }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      },
      "routes":[{
        "dest": "server.js"
      }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Vercel-Forwarded-For",
          "value": ":ip"
        }
      ]
    }
  ]
}